docker run -t -v "${PWD}:/data" osrm/osrm-backend osrm-extract -p /opt/car.lua /data/idf.osm.pbf
docker run -t -v "${PWD}:/data" osrm/osrm-backend osrm-partition /data/idf.osrm
docker run -t -v "${PWD}:/data" osrm/osrm-backend osrm-customize /data/idf.osrm
docker run -d -p 5000:5000 -v "${PWD}:/data" osrm/osrm-backend osrm-routed --algorithm mld /data/idf.osrm